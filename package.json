  "scripts": {
    "ng": "ng",
    "start": "node --max-old-space-size=12192 ./node_modules/@angular/cli/bin/ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ssr": "ng build && ng run info-gen:server",
    "serve:ssr": "node dist/info-gen/server/main.js",
    "build:client-and-server-bundles": "node --max-old-space-size=12192 ./node_modules/@angular/cli/bin/ng build --prod && ng run angular.io-example:server",
    "prod": "node --max-old-space-size=12192 ./node_modules/@angular/cli/bin/ng build --aot --prod --buildOptimizer",
    "fix-memory-limit": "cross-env LIMIT=12192 increase-memory-limit",
    "source": "node --max-old-space-size=12192 ./node_modules/@angular/cli/bin/ng build --prod --source-map",
    "stats": "node --max-old-space-size=12192 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
    "analyze": "webpack-bundle-analyzer dist/odisdkp/stats.json",
    "sanalyze": "source-map-explorer dist/odisdkp/main*js",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "dev:ssr": "ng run info-gen:serve-ssr",
    "prerender": "ng run info-gen:prerender"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1303",
    "@angular/cli": "^13.3.5",
